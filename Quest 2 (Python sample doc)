print("")
choices = [""]
#Note you have to type in the exact same gear as provided
while True:
  print ("Note you have to type in the exact same gear as provided",choices) 
  userschoice = str(input(".\n"))
  if userschoice == choices[0]:
    print("")
  elif userschoice == choices[1]:
    print("")
  else:
    print("You have to choose, or else you might not live to see tommorrow.")
  break


